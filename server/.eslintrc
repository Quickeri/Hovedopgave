{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "import/extensions": [
            "error",
            "never",
            { "js": "ignorePackages" }
        ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "plugins": ["eslint-plugin-prettier", "eslint-plugin-import"],
    "env": {
        "es6": true
    }
}
