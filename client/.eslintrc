{
	"extends": [
		"airbnb-base",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		// Allows the use of ++ and --
		"no-plusplus": "off",
		"react/prop-types": 0,
		"no-console": "off"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"plugins": ["eslint-plugin-prettier"],
	"env": {
		"es6": true,
		"browser": true
	},
	"settings": {
		"import/resolver": {
			"webpack": {
				"config": "webpack/base.config.js"
			}
		}
	}
}
